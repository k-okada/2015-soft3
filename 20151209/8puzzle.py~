#!/usr/bin/env python
class 8puzzle:
    def __init__(self,puzzle):
        self.puzzle = puzzle
        self.x,self.y = self.find(0)
        self.column_size = len(a)
        self.row_size = len(a[0])

    def up(self):
        if y <= 0:
            return
        yold = self.y

    def find(x):
        for column in range(0,column_size):
            for row in range(0,row_size):
                if self.puzzle[column][row] = x:
                    return column, row
        return False,False
                    
